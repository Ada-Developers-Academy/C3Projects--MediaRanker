<table>
  <thead>
    <th>Ranking</th>
    <th>Name</th>
    <th>Upvote</th>
  </thead>
  <tbody>
  <% @books.each do |book| %>
    <tr>
      <td>Ranked: <%= book.rank %></td>
      <td><%= link_to "#{book.name}", book_path(book.id) %></td>
      <td><%= button_to "Upvote" %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<%= link_to "View All Media", root_path %>
