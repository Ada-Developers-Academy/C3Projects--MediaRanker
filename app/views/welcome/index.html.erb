<div class="index_column">
  <h1> Top Movies </h1>
  <% @movies.each do |movie| %>
    <h2> <%= movie.name %>
      <small> ranked <%= movie.rank %> </small>
    </h2>
  <% end %>
</div>

<div class="index_column">
  <h1> Top Books </h1>
  <% @books.each do |book| %>
    <h2> <%= book.name %>
      <small> ranked <%= book.rank %> </small>
    </h2>
  <% end %>
</div>

<div class="index_column">
  <h1> Top Albums </h1>
  <% @albums.each do |album| %>
    <h2> <%= album.name %>
      <small> ranked <%= album.rank %> </small>
    </h2>
  <% end %>
</div>
