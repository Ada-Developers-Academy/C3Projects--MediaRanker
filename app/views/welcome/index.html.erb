<h1>Media Ranker</h1>

<table>
  <tr>
    <th>Top Movies</th>
  </tr>

  <% @movies.each do |movie| %>
  <tr><td><%= movie.name  %></td><tr>
  <% end %>

  <tr>
  <th>Top Books</th>
  </tr>

  <% @books.each do |book| %>
  <tr><td><%= book.name  %></td><tr>
  <% end %>

  <tr>
  <th>Top Albums</th>
  </tr>

  <% @albums.each do |album| %>
  <tr><td><%= album.name  %></td><tr>
  <% end %>


</table>
