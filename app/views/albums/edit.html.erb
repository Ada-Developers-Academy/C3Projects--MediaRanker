<%= render "shared/header" %>
<%= form_for @album, url: album_path, method: :patch do |f| %>
  <fieldset>
    <%= f.label :name %>
    <%= f.text_field :name, :required => true %>

    <%= f.label :artist %>
    <%= f.text_field :artist %>

    <%= f.label :description%>
    <%= f.text_field :description %>


  </fieldset>
    <%= f.submit 'Save' %>
<% end %>
