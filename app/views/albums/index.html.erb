<table>
  <thead>
    <th>Ranking</th>
    <th>Name</th>
    <th>Upvote</th>
  </thead>
  <tbody>
  <% @albums.each do |album| %>
    <tr>
      <td>Ranked: <%= album.rank %></td>
      <td><%= link_to "#{album.name}", album_path(album.id) %></td>
      <td><%= button_to "Upvote" %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<%= link_to "View All Media", root_path %>
