<h1><%= @medium.name %></h1>
<small><%= byline + @medium.contributor %></small>
<h4>Ranked: <%= @medium.ranking %></h4>
<p><%= @medium.description %></p>
<%= button_to 'Upvote', upvote_medium_path %>


<%= button_to 'Edit ' + @medium.name, edit_medium_path, method: "get" %>
<%= button_to "Delete", medium_path(@medium.id, :from => @medium.media_type), method: "delete", data: { confirm: "Are you sure?" } %>
