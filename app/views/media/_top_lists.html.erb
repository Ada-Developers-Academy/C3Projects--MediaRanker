<div class="col-md-4">
  <h1>Top <%= title %> </h1>
    <ul>
      <% media.each do |item| %>
        <li><%= link_to item.name, polymorphic_path(item) %> Ranked: <%= item.ranking %></li>
      <% end %>
    <%= link_to "View More #{title}", polymorphic_path("#{title.downcase}_index") %>
    </ul>
</div>
