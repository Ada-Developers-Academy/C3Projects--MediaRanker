<table>
  <thead>
    <tr>
      <th>Ranking</th>
      <th>Name</th>
      <th>Upvote</th>
    </tr>
  </thead>
  <% media.each do |object| %>
    <tr>
      <td>Ranked:<%= object.votes %></td>
      <td><%= object.title %></td>
      <td><%= button_to "Upvote", object, method: :put, params: { upvote: true }%></td>
    </tr>
  <% end %>
</table>
