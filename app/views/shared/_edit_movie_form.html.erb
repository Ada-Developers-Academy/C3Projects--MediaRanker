<%= form_for @movie, url: movie_path, method: :patch do |f| %>
  <fieldset>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :director %>
    <%= f.text_field :director %>

    <%= f.label :description%>
    <%= f.text_field :description %>

    <%= f.label :rank %>
    <%= f.text_field :rank %>

  </fieldset>
    <%= f.submit 'Save' %>
<% end %>
