<%= form_for @album, url: album_path, method: :patch do |f| %>
  <fieldset>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :artist %>
    <%= f.text_field :artist %>

    <%= f.label :description%>
    <%= f.text_field :description %>

    <%= f.label :rank %>
    <%= f.text_field :rank %>

  </fieldset>
    <%= f.submit 'Save' %>
<% end %>
