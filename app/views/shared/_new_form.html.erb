<%= form_for subject, url: path do |f| %>
    <legend>New <%= "#{subject.class}" %></legend>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label by %>
    <%= f.text_field by %>

    <%= f.label :description %>
    <%= f.text_area :description %>

 <%= f.submit 'Save' %>
<% end %>
