
<%= form_for object do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name, required: :true %>

  <%= f.label object_creator %>
  <%= f.text_field object_creator, required: :true %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.submit "Save", class: "btn btn-default" %>

<% end %>
