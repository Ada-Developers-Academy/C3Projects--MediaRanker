<table>
  <tr>
    <th>Ranking</th>
    <th>Name</th>
    <th>Upvote</th>
  </tr>

    <% types.each do |type| %>
    <tr>
      <td><%= type.rank %></td>
      <td><%= type.name %></td>

      <td>
      <%= form_for type do |f| %>
        <%= f.submit "Upvote" %>
      <% end %>
      </td>


    </tr>
    <% end %>

</table>

<%= button_to "View All Media", root_path, method: :get %>

