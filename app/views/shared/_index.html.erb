<%= render "shared/header" %>


<table class="table table-hover">
    <tr>
      <th>Ranking</th>
      <th>Name</th>
      <th>Upvote</th>
    </tr>

    <% objects.each do |object|%>
    <tr>
      <td>Ranked: <%= object.rank %></td>
      <td><%= link_to object.name, polymorphic_path(object) %></td>
      <td><%= link_to "Upvote", polymorphic_path([:upvote, object]), method: :post, class: "btn btn-default" %></td>
    </tr>
    <% end %>


</table>

<%= link_to "View All Media", root_path, class: "btn btn-default" %>
<%= link_to "Add an Album", "/#{object_path}/new", class: "btn btn-default" %>





