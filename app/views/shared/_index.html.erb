<div class="row">
  <div class="col-md-12">
    <table class="table table-hover">
        <tr>
          <th>Ranking</th>
          <th>Name</th>
          <th>Upvote</th>
        </tr>
      <% medium.each do |object| %>
        <tr>
          <td>Ranked:<%= object.votes %></td>
          <td><%= link_to "#{object.title}", polymorphic_path(object) %></td>
          <td><%= button_to "Upvote", object, method: :put, class: "btn btn-default",
            params: { upvote: "true" }%></td>
        </tr>
      <% end %>
    </table>

    <%= link_to "View All Media", root_path, class: "btn btn-default" %>
    <%= link_to "Add #{medium_name}", new_polymorphic_path(medium_class), class: "btn btn-default" %>
  </div>
</div>
