<div class="col-lg-12">
    <table class="table table-hover">
      <tbody>
      <tr>
        <th>Ranking</th>
        <th>Name</th>
        <th>Upvote</th>
      </tr>
    </tbody>

    <tbody>

      <% @movies.each do |movie| %>
      <tr>
        <td>Ranked: <%= movie.rank %></td>
        <td><%= link_to "#{movie.name}", movie_path(movie.id) %></td>
        <td><%= link_to "Upvote", upvote_movie_path(movie.id), method: :patch, class: "btn btn-default" %></td>
      </tr>
      <% end %>

    <tbody>
  </table>
    <p> <%= link_to "View All Media", root_path, method: :get, class: "btn btn-default" %> </p>
    <p> <%= link_to "Add a Movie", new_movie_path , method: :get, class: "btn btn-default" %> </p>
</div>
